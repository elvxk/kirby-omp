          "style": "plain",
          "foreground": "p:blue",
          "properties": {
            "time_format": "[15:04]"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:pink{{ end }}"],
          "template": "\u2570\u2500",
          "properties": {
            "always_enabled": true
          },
          "type": "status"
        },
        {
          "foreground": "p:os",
          "foreground_templates": ["{{ if gt .Code 0 }}p:pink{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\uf105 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
