{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",

  "console_title_template": "Kirby üêæ {{.Folder}}",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA"
  },

  "blocks": [
    {
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "foreground_templates": ["{{ if gt .Code 0 }}p:pink{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u256d\u2500ÔÜ∞ ",
          "type": "status"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "foreground": "p:pink",
          "properties": {
            "hide_root_location": true,
            "folder_separator_icon": " ",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "\uf105 {{ if eq .PWD `~` }}\uf198 {{ else if hasPrefix (printf \"%.2s\" .PWD) `~\\` }}\uf198 {{.Folder}} {{else}}\ue5fe {{.Folder}} {{end}}\uf105 ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} Ôöí {{ .StashCount }}{{ end }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "p:os",
          "properties": {
            "style": "round",
            "threshold": 0
          },
          "style": "plain",
          "template": "{{ .FormattedMs }}",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "angular",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "Óùì "
        },
        {
          "type": "dart",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "Óûò "
        },
        {
          "type": "docker",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "Ôåà  "
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "Óùø  "
        },
        {
          "type": "flutter",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "Óäé "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "Óòß "
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "Óú∏ "
        },
        {
